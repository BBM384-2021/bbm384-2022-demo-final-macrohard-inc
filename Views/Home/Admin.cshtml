<header>
    <div>
        <button class="meButton">Me</button>
    </div>
</header>

<body>
<div class="notifications">
    @foreach (var notification in ViewBag.Notifications)
    {
        <p>@notification.NotificationContent</p>
        <p>Registration Date: @notification.NotificationTime</p>

    }
</div>

<div class="accounts">
        

       
        <a href=@Url.ActionLink("ExportToExcel","Admin")>Download User Details</a>  

        @using (Html.BeginForm("SearchUser", "Admin", FormMethod.Get))
        {
            @Html.TextBox("searchKey")<input type="submit" value="Search"/>
            
        
        }
        @if(ViewBag.UserList.Count==0){
            <p>No Matching</p>
        }else{
            @foreach (var user in ViewBag.UserList)
        {
            
                @using (Html.BeginForm("DeleteUser", "Admin", new { AccountId = user.AccountId }, FormMethod.Post))
                {

                    <p>@user.AccountId</p>
                    <input type="submit" value="Delete"/>
                }
                <button onclick="closePop()">Show details</button>
                <div class="userDetails" id="detail" style="visibility: hidden">
                    <p>@user.FirstName</p>
                    <p>@user.LastName</p>
                    <p>@user.AccountId</p>
                    <p>@user.PhoneNumber</p>
        </div>
        }
        
         
    
        }
        
    
